class IShapePiece is subclass of PivotPiece

operations
	public IShapePiece: int * int * char ==> IShapePiece
	IShapePiece(x, y, s) == (
		PivotPiece(x, y, <I>, s);
	);

	public rotate: () ==> ()
	rotate() == (
		rotation();
		cases currentRotation:
			3 -> move(1,-1),
			4 -> move(-1,-1)
		end;
	);

end IShapePiece